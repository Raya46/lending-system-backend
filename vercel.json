{
    "version":2,
    "builds": [{"src":"./express.js", "use":"@vercel/node"}],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/express.js"
        }
    ],
    "functions": {
        "./express.js": {
            "maxDuration": 30
        }
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}