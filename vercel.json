{
    "version": 2,
    "functions": {
        "express.js": {
            "maxDuration": 30,
            "runtime": "nodejs18.x"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/express.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}